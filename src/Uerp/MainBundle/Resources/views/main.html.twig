    <script>
$(document).ready(function() {
 
        $('.submitOnChange').change( function(event) {
    $.ajax({
        type: "GET",
        data: "data=" + $(this).val(),
        url:"{{ path('_listsubcategories') }}",
        success: function(msg){
            if (msg != ''){
               $('.ssubcategories').html(msg).show();
               }
               else
               {
                   $('.ssubcategories').html('<em>No item result</em>');
               }
           }
       });
  });
});
</script>
